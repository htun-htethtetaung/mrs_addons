<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_mrs_doctor_speciality_form" model="ir.ui.view">
        <field name="name">MRS Doctor Speciality</field>
        <field name="model">mrs.doctor.speciality</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_mrs_doctor_speciality_list" model="ir.ui.view">
        <field name="name">MRS Doctor Speciality</field>
        <field name="model">mrs.doctor.speciality</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="mrs_doctor_speciality_action" model="ir.actions.act_window">
        <field name="name">MRS Doctor Speciality</field>
        <field name="res_model">mrs.doctor.speciality</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
            Add a new MRS Doctor Speciality
            </p><p>
            Create MRS Doctor Speciality
            </p>
        </field>
    </record>
</odoo>
